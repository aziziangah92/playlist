<body>
    <div class="password-container" id="passwordContainer">
        <div class="password-box">
            <h2>Masukkan Kata Laluan</h2>
            <input type="password" id="passwordInput" placeholder="Kata Laluan">
            <br>
            <button onclick="checkPassword()">Hantar</button>
        </div>
    </div>
    <div class="main-menu hidden" id="mainMenu">
        <h1>Selamat Datang ke OTT Player</h1>
        <!-- Kandungan menu lain boleh ditambah di sini -->
        <div class="shop-name">Beli di kedai Shopee NeckleNj</div>
    </div>
    <script>
        const correctPassword = "95764"; // Tukar kepada kata laluan yang dikehendaki

        function checkPassword() {
            const input = document.getElementById("passwordInput").value;
            if (input === correctPassword) {
                document.getElementById("passwordContainer").classList.add("hidden");
                document.getElementById("mainMenu").classList.remove("hidden");
            } else {
                alert("Kata laluan salah. Sila cuba lagi.");
            }
        }

        // Benarkan kemasukan kata laluan dengan kekunci Enter
        document.getElementById("passwordInput").addEventListener("keypress", function(event) {
            if (event.key === "Enter") {
                checkPassword();
            }
        });
    </script>
</body>
